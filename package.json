    "@vitejs/plugin-react": "4.2.1",
    "electron": "28.2.0",
    "electron-builder": "24.9.1",
    "electron-vite": "2.0.0",
    "prettier": "3.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.2",
    "sass": "1.75.0",
    "typescript": "5.3.3",
    "vite": "5.0.12"
  }
}
